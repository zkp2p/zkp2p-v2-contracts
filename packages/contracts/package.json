{
  "name": "@zkp2p/contracts-v2",
  "version": "0.0.1-rc1",
  "description": "ZKP2P V2 smart contract interfaces and utilities",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn extract && yarn bundle",
    "build:full": "yarn build",
    "clean": "rimraf dist",
    "extract": "ts-node --transpile-only scripts/extract-all.ts",
    "compile": "echo 'Skipping tsc, using Rollup for compilation'",
    "bundle": "rollup -c --bundleConfigAsCjs",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "prepublishOnly": "yarn build && yarn test"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.esm.js",
      "types": "./dist/index.d.ts"
    },
    "./addresses": {
      "default": "./dist/addresses/index.json",
      "types": "./dist/addresses/index.d.ts"
    },
    "./addresses/*.json": {
      "default": "./dist/addresses/*.json"
    },
    "./abis/*/": {
      "default": "./dist/abis/*/*.json",
      "types": "./dist/abis/*/index.d.ts"
    },
    "./constants": {
      "types": "./dist/constants/index.d.ts"
    },
    "./constants/*.json": {
      "default": "./dist/constants/*.json"
    },
    "./paymentMethods": {
      "default": "./dist/paymentMethods/index.json",
      "types": "./dist/paymentMethods/index.d.ts"
    },
    "./paymentMethods/*.json": {
      "default": "./dist/paymentMethods/*.json"
    },
    "./types": {
      "types": "./dist/types/index.d.ts"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts"
    },
    "./utils/protocolUtils": {
      "types": "./dist/utils/protocolUtils.d.ts"
    }
  },
  "sideEffects": false,
  "peerDependencies": {
    "ethers": "^5.0.0 || ^6.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "ethers": "^5.7.2",
    "jest": "^29.0.0",
    "rimraf": "^5.0.0",
    "rollup": "^3.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zkp2p/zkp2p-v2-contracts.git",
    "directory": "packages/contracts"
  },
  "keywords": [
    "zkp2p",
    "ethereum",
    "smart-contracts",
    "defi",
    "p2p",
    "fiat",
    "onramp",
    "offramp"
  ],
  "author": "ZKP2P",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zkp2p/zkp2p-v2-contracts/issues"
  },
  "homepage": "https://zkp2p.xyz",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
